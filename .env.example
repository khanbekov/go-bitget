# Bitget API Configuration
# Copy this file to .env and fill in your actual API credentials

# API Credentials (Required for integration tests)
BITGET_API_KEY=your_api_key_here
BITGET_SECRET_KEY=your_secret_key_here
BITGET_PASSPHRASE=your_passphrase_here

# Environment Settings
BITGET_DEMO_TRADING=true
BITGET_TESTNET=false

# Base URL (optional - defaults to production)
# BITGET_BASE_URL=https://api.bitget.com

# For testnet usage (uncomment if needed)
# BITGET_TESTNET=true
# BITGET_BASE_URL=https://testnet.bitget.com